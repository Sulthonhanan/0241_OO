 #include <iostream>
using namespace std;

class Mahasiswa {
    public: //akses modifier
        string nama;
        int umur;
        string jurusan;

        void output() {
            cout << "Nama: " << nama << endl;
            cout << "Umur: " << umur << endl;
            cout << "Jurusan" << jurusan << endl;

        }
};


 class Matakuliah{
    private:
        string kodeMk;
        string namaMk;
        int sks;
    public:
}

        void output(){
            cout << "KodeMk: " << kodeMk << endl;
            cout << "NamaMk: " << namaMk << endl;
            cout << "SKS: " << sks << endl;
         }
  };
